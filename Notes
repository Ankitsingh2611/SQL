@echo off

For /f "tokens=1-2 delims=/:" %%a in ("%TIME%") do (set mytime=%%a%%b)


set usr=root
set pwd=
set backupdir="C:\mysqlAutoBackup\backup"
set dbname=carservcrud
set dmpdir="C:\xampp824\mysql\bin\mysqldump.exe"

%dmpdir% --host=localhost --user=%usr% --password=%pwd% %dbname% > %backupdir%\%dbname%%mytime%.sql 2> %backupdir%\%dbname%%mydate%_%mytime%.log


\end{code}  




